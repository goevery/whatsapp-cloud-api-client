{
  "name": "@goevery/whatsapp-cloud-api-client",
  "version": "0.2.0",
  "description": "A TypeScript client for WhatsApp Cloud API",
  "type": "module",
  "license": "MIT",
  "private": false,
  "homepage": "https://github.com/goevery/whatsapp-cloud-api-client#readme",
  "bugs": {
    "url": "https://github.com/goevery/whatsapp-cloud-api-client/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goevery/whatsapp-cloud-api-client.git"
  },
  "author": "Juan Mar√≠n <juan@goevery.co>",
  "files": [
    "dist"
  ],
  "main": "./dist/main.cjs",
  "module": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.js",
      "require": "./dist/main.cjs"
    },
    "./schemas": {
      "types": "./dist/schemas.d.ts",
      "import": "./dist/schemas.js",
      "require": "./dist/schemas.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/node": "^24.9.2",
    "bumpp": "^10.3.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "jiti": "^2.6.1",
    "prettier": "^3.6.2",
    "tsdown": "^0.15.11",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vitest": "^4.0.4"
  },
  "dependencies": {
    "zod": "^4.1.12"
  },
  "packageManager": "pnpm@10.20.0+sha512.cf9998222162dd85864d0a8102e7892e7ba4ceadebbf5a31f9c2fce48dfce317a9c53b9f6464d1ef9042cba2e02ae02a9f7c143a2b438cd93c91840f0192b9dd"
}
